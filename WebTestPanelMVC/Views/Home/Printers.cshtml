@model AllListModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

@*PRINTERS TABLE*@
<div class="row  ">
    
    <div class="col-12"><h1 class="display-4 text-center fs-5 fw-bold">Yazıcılar Tablosu</h1>
        <table class="table table-bordered table-dark table-hover  m-3 p-2">
            <thead>
            <tr>
               
                <th scope="col">Yazıcı ID</th>
                <th scope="col">Marka</th>
                <th scope="col">Model </th>
                <th scope="col">Yazıcı IP </th>
                <th scope="col">Kordinatı </th>
            </tr>
            </thead>
            <tbody>
            @foreach (var item in Model.Printers)
            {
                <tr>
                   
                    <td>@item.Id</td>
                    <td>@item.Brand</td>
                    <td>@item.Model</td>
                    <td>@item.PrinterIP</td>
                    <td>@item.Coordinate</td>
                   


                </tr>
            }
       
            </tbody>
        </table>
    
    </div>
   
</div>

