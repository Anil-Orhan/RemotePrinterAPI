@model AllListModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

@*FILES TABLE*@
<div class="row">
    
    <div class="col-12"><h1 class="display-4 text-center fs-1 fw-bolder mt-4 mb-4 fw-bold">Files</h1>
       
        <table class="table table-bordered table-dark fs-5">
            <thead>
            <tr>
               
                <th scope="col">ID</th>
                <th scope="col">User ID</th>
                <th scope="col">File URL</ </th>
                <th scope="col">Cloud URL </th>
                <th scope="col">File Name </th>
                <th scope="col">Extension </th>
                <th scope="col">Size </th>
                <th scope="col">Date </th>
                <th scope="col">Operation ID </th>
              
            </tr>
            </thead>
            <tbody>
            @foreach (var item in Model.Files)
            {
                <tr>
                   
                    <td>@item.Id</td>
                    <td>@item.UserId</td>
                    <td>@item.FileUrl</td>
                    <td>@item.CloudUrl</td>
                    <td>@item.FileName</td>
                    <td>@item.FileExtension</td>
                    <td>@item.FileSize</td>
                    <td>@item.CreateDate</td>
                    <td>@item.OperationId</td>


                    


                </tr>
            }
       
            </tbody>
        </table>
        
    </div>
   
</div>
