@model AllListModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="p-5 m-3">
    
    <h1 class="fs-1 text-center">Tables</h1>
    
    @*PRINTERS TABLE*@
    <div class="row">
    
        <div class="col-12"><h1 class="display-4 text-center fs-5 fw-bold">Printers</h1>
            <table class="table table-bordered m-3 p-2">
                <thead>
                <tr>
               
                    <th scope="col">Id</th>
                    <th scope="col">Brand</th>
                    <th scope="col">Model </th>
                    <th scope="col">PrinterIP </th>
                    <th scope="col">Coordinate </th>
                </tr>
                </thead>
                <tbody>
                @foreach (var item in Model.Printers)
                {
                    <tr>
                   
                        <td>@item.Id</td>
                        <td>@item.Brand</td>
                        <td>@item.Model</td>
                        <td>@item.PrinterIP</td>
                        <td>@item.Coordinate</td>


                    </tr>
                }
       
                </tbody>
            </table>
    
        </div>
   
    </div>
    <hr class="dashed"/>
    @*FILES TABLE*@
    <div class="row">
    
        <div class="col-12"><h1 class="display-4 text-center fs-5 fw-bold">Files</h1>
            <table class="table table-bordered m-3 p-2">
                <thead>
                <tr>
               
                    <th scope="col">Id</th>
                    <th scope="col">UserId</th>
                    <th scope="col">FileUrl</ </th>
                    <th scope="col">CloudUrl </th>
              
                </tr>
                </thead>
                <tbody>
                @foreach (var item in Model.Files)
                {
                    <tr>
                   
                        <td>@item.Id</td>
                        <td>@item.UserId</td>
                        <td>@item.FileUrl</td>
                        <td>@item.CloudUrl</td>
                    


                    </tr>
                }
       
                </tbody>
            </table>
    
        </div>
   
    </div>

    @*USERS TABLE*@

    <div class="row">
    
        <div class="col-10"><h1 class="display-4 text-center fs-5 fw-bold">Active Users</h1>
            <table class="table table-bordered m-3 p-2">
                <thead>
                <tr>
                   
                    <th scope="col">Full Name</th>
                    <th scope="col">Phone Number</th>
                    <th scope="col">User Name </th>
                </tr>
                </thead>
                <tbody>
                @foreach (var item in Model.Users)
                {
                    <tr>
                        <td>@item.name @item.lastName</td>
                        <td>@item.phoneNumber</td>
                        <td>@item.userName</td>
                

                    </tr>
                }
       
                </tbody>
            </table>
    
        </div>


    </div>
    
    @*PRINT MODEL TABLE*@

    <div class="row">
    
        <div class="col-10"><h1 class="display-4 text-center fs-5 fw-bold">PRINTS</h1>
            <table class="table table-bordered m-3 p-2">
                <thead>
                <tr>
                   
                    <th scope="col">ID</th>
                    <th scope="col">USER ID</th>
                    <th scope="col">PRINTER ID </th>
                    <th scope="col">FILE ID </th>
                    <th scope="col">DATE</th>

                </tr>
                </thead>
                <tbody>
                @foreach (var item in Model.PrintModels)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.UserId</td>
                        <td>@item.PrinterId</td>
                        <td>@item.FileId</td>
                        <td>@item.Date</td>
                

                    </tr>
                }
       
                </tbody>
            </table>
    
        </div>


    </div>



</div>


