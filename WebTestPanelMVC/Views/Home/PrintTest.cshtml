@model AllListModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="row text-center" >

<form method="post" enctype="multipart/form/data" asp-controller="Home" asp-action="PrintTest" class="text-center" style="text-align: center">
    
   
    <div class="form-group row ">
        <label class="fs-2 mt-5 pt-5 fw-light" for="fileInput">
            Select the file to upload </label>
        <input type="file" name="files" class="form-control-file" id="fileInput">
    </div>
   
 
    
    <button type="submit"  value="Upload" class="btn btn-success mb-2 mt-5 px-5 py-2"><i class="fa-solid fa-print"></i> Print</button>
</form>
</div>
