@model AllListModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="row text-center" >

<form asp-form class="text-center" style="text-align: center">
    
   
    <div class="form-group row ">
        <label class="fs-2 mt-5 pt-5 fw-light" for="exampleFormControlSelect1">
            Select the file to upload </label>
        <input type="file" class="form-control-file" id="exampleFormControlFile1">
    </div>
    <div class="row- text-center"> 
        <div class="form-group col-2  ">
            <label class="fw-light" for="exampleFormControlSelect1">
                Select print quantity</label>
            <select class="form-control" id="exampleFormControlSelect1">
                <option>1</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                <option>5</option>
            </select>
        </div></div>
  
    <div class="form-check form-switch">
        <div class="row ">
            <div class="col"> <input class="form-check-input shadow-none" type="checkbox" role="switch" id="flexSwitchCheckDefault">
                <label class="form-check-label fw-light" for="flexSwitchCheckDefault">Colored</label></div>


        </div>

    </div>
    
    <button type="submit" class="btn btn-success mb-2 mt-5 px-5 py-2"><i class="fa-solid fa-print"></i> Print</button>
</form>
</div>
