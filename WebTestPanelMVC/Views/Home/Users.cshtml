@model AllListModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}


@*USERS TABLE*@

<div class="row  ">
    
    <div class="col-10"><h1 class="display-4 text-center fs-5 fw-bold">Kullanıcılar Tablosu </h1>
        <table class="table table-bordered m-3 p-2 table-hover table-dark">
            <thead>
            <tr>
                   
                <th scope="col">Tam Ad</th>
                <th scope="col">Telefon Numarası</th>
                <th scope="col">Kullanıcı Adı </th>
            </tr>
            </thead>
            <tbody>
            @foreach (var item in Model.Users)
            {
                <tr>
                    <td>@item.name @item.lastName</td>
                    <td>@item.phoneNumber</td>
                    <td>@item.userName</td>
                

                </tr>
            }
       
            </tbody>
        </table>
    
    </div>


</div>
