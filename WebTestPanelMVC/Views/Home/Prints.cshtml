@model AllListModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

@*PRINT MODEL TABLE*@

<div class="row ">
    
    <div class="col-10"><h1 class="display-4 text-center fs-5 fw-bold">Yazdırma İşlemleri Tablosu</h1>
        <table class="table table-bordered table-dark table-hover m-3 p-2">
            <thead>
            <tr>
                   
                <th scope="col">ID</th>
                <th scope="col">Kullanıcı ID</th>
                <th scope="col">Yazıcı ID </th>
                <th scope="col">Dosya ID </th>
                <th scope="col">Tarih</th>

            </tr>
            </thead>
            <tbody>
            @foreach (var item in Model.PrintModels)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.UserId</td>
                    <td>@item.PrinterId</td>
                    <td>@item.FileId</td>
                    <td>@item.Date</td>
                

                </tr>
            }
       
            </tbody>
        </table>
    
    </div>


</div>

